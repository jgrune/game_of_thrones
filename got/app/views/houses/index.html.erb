<h1>Houses <%= link_to "(+)", new_house_path %></h1>
<main>


<% @houses.each do |house| %>
  <div class="got">
    <h2>
      <%= link_to house.name, house_path(house.id) %>
      <%= link_to "(-)", house_path(house.id), method: :delete %>
    </h2>
    <img src="<%= house.img_url %>">
  </div>
<% end %>
</main>
